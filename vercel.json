{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/api/oauth/start$",
      "dest": "/api/oauth/start.js"
    },
    {
      "src": "^/api/oauth/callback$",
      "dest": "/api/oauth/callback.js"
    },
    {
      "src": "^/api/oauth/refresh$",
      "dest": "/api/oauth/refresh.js"
    },
    {
      "src": "^/api/?$",
      "dest": "/api/index.js"
    }
  ]
}
